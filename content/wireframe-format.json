                                            {
                                                "element-type": "form",
                                                "type": "vertical | horizontal"
                                            },
                                            {
                                                "element-type": "social-links",
                                                "links-count": 3,
                                                "align": "left | right | center" // center by default
                                            },
                                            {
                                                "element-type": "box",
                                                "type": "vertical | horizontal",
                                                "widgets": [],
                                                "background": "hex | image"
